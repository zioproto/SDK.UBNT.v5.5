+unsigned int gpio_line_set(unsigned char line, u32 val)
+{
+	ar7240_reg_t reg = ar7240_reg_rd(AR7240_GPIO_OUT);
+
+	reg &= ~(1 << line);
+	reg |= (val & 1) << line;
+	ar7240_reg_wr_nf(AR7240_GPIO_OUT, reg);
+	return ar7240_reg_rd(AR7240_GPIO_OUT);
+}
