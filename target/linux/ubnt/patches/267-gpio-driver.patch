+static u32 gpio_leds_set(u8* leds)
+{
+	ar7240_reg_t reg_out = 0;
+	u8 c, n, d, o, i;
+
+	if (!leds || !(c = leds[0]))
+	{
+		return 0;
+	}
+
+	for (i = 1; i <= c; ++i)
+	{
+		n = leds[i] >> 2;
+		d = (leds[i] >> 1) & 1;
+		o = leds[i] & 1;
+#ifdef DEBUG
+		printk("GPIO_OE[%d] = %d, GPIO_OUT[%d] = %d (0x%02X)\n",
+				n, d, n, o, leds[i]);
+#endif
+		gpio_line_config(n, d);
+		gpio_line_set(n, o);
+	}
+
+	return reg_out;
+}
